.panel {
  width: 800px;
  height: 300px;
  background-color: #333;
  border-radius: 6px;
  flex-direction: column;
  padding: 6px;
  position: absolute;
}

.header {
  background-color: #444;
  height: 30px;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  -unity-font-style: bold;
  cursor: move;
}
