.panel {
    width: 800px;
    height: 300px;
    background-color: rgb(51, 51, 51);
    border-radius: 6px;
    flex-direction: column;
    padding: 6px;
    position: absolute;
    flex-shrink: 0;
    flex-basis: initial;
}

.header {
    background-color: rgb(68, 68, 68);
    height: 30px;
    flex-direction: row;
    justify-content: space-between;
    -unity-font-style: bold;
}
